<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--在此处修改默认打开的串口号以及波特率-->
    <add key="串口号" value="COM2" />
    <add key="波特率" value="9600" />
    
    <!--在此处修改需要添加的衰减、移相、延迟-->
    <!--模式为DEBUG时，自动添加所有衰减、移相、延迟等参数；模式为RELEASE时，则添加以下配置中的参数-->
    <add key="模式" value="DEBUG" />
    
    <add key="衰减" value="0dB;0.5dB;1dB;2dB;4dB;8dB;16dB" />
    <add key="移相" value="0°;5.625°;11.25°;22.5°;45°;90°;180°" />
    <add key="延迟" value="0λ;1λ;2λ;4λ;8λ;16λ" />
    
    
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
